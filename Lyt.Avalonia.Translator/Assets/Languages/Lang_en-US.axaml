	<system:String x:Key="Projects.None">No saved projects were found.</system:String>
	<system:String x:Key="Projects.ProjectDeleted">Project was successfully deleted.</system:String>

	<!-- Run Project  -->
	<system:String x:Key="RunProject.NoActiveProject">No Active Project.</system:String>
	<system:String x:Key="RunProject.Run">Run</system:String>
	<system:String x:Key="RunProject.Stop">Abort</system:String>
	<system:String x:Key="RunProject.Idle">Idle. Not Translating.</system:String>
	<system:String x:Key="RunProject.InProgress">Translation in progress...</system:String>

