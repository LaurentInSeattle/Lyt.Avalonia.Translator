	<system:String x:Key="CreateNew.AddAllLanguages">All Languages</system:String>
	<system:String x:Key="CreateNew.ClearAllLanguages">Remove All</system:String>
	<system:String x:Key="CreateNew.SaveProject">Save Project</system:String>

