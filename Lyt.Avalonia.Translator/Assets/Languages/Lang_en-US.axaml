	<system:String x:Key="CreateNew.UnsupportedFileFormat">Unsupported File Format</system:String>
	<system:String x:Key="CreateNew.UnsupportedCulture">Unsupported Culture</system:String>
	<system:String x:Key="CreateNew.Untitled">- Untitled -</system:String>
	<system:String x:Key="CreateNew.CantSaveProjectTitle">Can't Save Project</system:String>
	<system:String x:Key="CreateNew.CantSaveProjectText">More information is required to save the project.</system:String>
	<system:String x:Key="CreateNew.ProjectSavedTitle">Saved.</system:String>
	<system:String x:Key="CreateNew.ProjectSavedText">This project has been successfully saved.</system:String>
	<system:String x:Key="Model.Project.AlreadyExists">A project with this name already exists.</system:String>

