	<system:String x:Key="Model.Project.Format">Resource file format is invalid or not supported.</system:String>
	<system:String x:Key="Model.Project.Name">This project has no name.</system:String>
	<system:String x:Key="Model.Project.FolderPath">The project directory is invalid.</system:String>
	<system:String x:Key="Model.Project.SourceFile">The project source file is invalid.</system:String>
	<system:String x:Key="Model.Project.TargetFileFormat">No target file format string.</system:String>
	<system:String x:Key="Model.Project.ZeroTargetLanguages">No target languages have been selected.</system:String>
	<system:String x:Key="Model.Project.InvalidName">Invalid project name.</system:String>
	<system:String x:Key="Model.Project.DoesNotExist">No project with such name.</system:String>

	<!-- Most likely all below will be deleted -->
	<!-- Keep for now, maybe there are some recycling opportunities -->

	<system:String x:Key="CreateNew.Remove">Remove</system:String>

	<system:String x:Key="Collection.SaveToDesktop">Save to Desktop</system:String>
	<system:String x:Key="Collection.Statistics">Statistics</system:String>
	<system:String x:Key="Collection.Stats.AlertTextSpace">Pay attention to the available space on disk!</system:String>
	<system:String x:Key="Collection.Stats.SizeOnDiskFormat">Estimated disk space usage: {0} MB (Quota: {1} MB)</system:String>
	<system:String x:Key="Collection.Stats.AvailableDiskSpaceFormat">Available space on disk: {0:F1} GB</system:String>

	<system:String x:Key="Collection.Thumbs.All">All</system:String>
	<system:String x:Key="Collection.Thumbs.Recent">Recent</system:String>

	<!-- Gallery  -->
	<system:String x:Key="Gallery.Add">Add</system:String>

	<!-- Intro  -->
	<system:String x:Key="Intro.AgreeMIT">I accept the terms of the MIT License, Continue.</system:String>
	<system:String x:Key="Intro.LicenseTitle">License Terms</system:String>
	<system:String x:Key="Intro.LicenseSubtitle">This application is provided to you under the terms of the MIT License.</system:String>
	<system:String x:Key="Intro.LicenseTerms">This application is provided to you under the terms of the MIT License and, together with other terms, implies no warranty of any kind. By using this software, you fully agree to the terms and conditions of the MIT License.</system:String>
	<system:String x:Key="Intro.ReminderTitle">Important Reminder</system:String>
	<system:String x:Key="Intro.ReminderSubtitle">Some important points to remember about downloaded images...</system:String>
	<system:String x:Key="Intro.ReminderText">You are advised to use the downloaded images only as wallpaper. For any other use, commercial or otherwise, please contact the copyright holder.</system:String>

	<!-- Settings  -->
	<system:String x:Key="Settings.CleanupNow">Cleanup Now</system:String>
	<system:String x:Key="Settings.Services.Skip">Ignore and Skip</system:String>

	<system:String x:Key="Settings.Quota.Title">Disc usage settings</system:String>
	<system:String x:Key="Settings.Quota.Subtitle">Use this setting to limit (or not limit) the use of space on your disc for the image in your collection.</system:String>
	<system:String x:Key="Settings.Quota.ImageCount">Maximum count of images:</system:String>
	<system:String x:Key="Settings.Quota.DiskSpace">Space quota (MB) on the disc for the images:</system:String>
	<system:String x:Key="Settings.Quota.ImageSize">Dimensions for image archiving:</system:String>
	<system:String x:Key="Settings.Quota.ImageSize.True">High Definition (HD)</system:String>
	<system:String x:Key="Settings.Quota.ImageSize.False">Ultra High Definition (UHD/4K)</system:String>
	<system:String x:Key="Settings.Quota.Exceeded">What should be done when quotas are exceeded:</system:String>
	<system:String x:Key="Settings.Quota.Exceeded.True">Delete old images.</system:String>
	<system:String x:Key="Settings.Quota.Exceeded.False">Only show a warning message.</system:String>

	<system:String x:Key="Settings.Os.Title">Operating System Settings</system:String>
	<system:String x:Key="Settings.Os.Subtitle">Choose some operating system settings to manage your images.</system:String>
	<system:String x:Key="Settings.Os.Rotate.True">Yes, periodically.</system:String>
	<system:String x:Key="Settings.Os.Rotate.False">Never</system:String>

	<system:String x:Key="Settings.ReminderTitle">Important Reminder</system:String>
	<system:String x:Key="Settings.ReminderSubtitle">Some important points to remember about downloaded images...</system:String>
	<system:String x:Key="Settings.ReminderText">You are advised to use the downloaded images only as wallpaper. For any other use, commercial or otherwise, please contact the copyright holder.</system:String>

	<!-- Shared  -->
	<system:String x:Key="Shared.NoImage">No image</system:String>
	<system:String x:Key="Shared.FileErrorTitle">File system error.</system:String>
	<system:String x:Key="Shared.FileErrorText">Could not save the image file.</system:String>

	<!-- Tray  -->
	<system:String x:Key="Tray.OpenCollection">Open the image collection</system:String>
	<system:String x:Key="Tray.Settings">Settings</system:String>
	<system:String x:Key="Tray.ImageInfo">About this image...</system:String>
	
