	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:system="clr-namespace:System;assembly=System.Runtime"
	>

	<!-- TextBox/SelectableTextBox flyout -->
	<x:String x:Key="StringTextFlyoutCutText">Cut</x:String>
	<x:String x:Key="StringTextFlyoutCopyText">Copy</x:String>
	<x:String x:Key="StringTextFlyoutPasteText">Paste</x:String>
	
	<!-- Toast-->
	<system:String x:Key="Toast.Dismiss">Close</system:String>

	<!-- Shell -->
	<system:String x:Key="Shell.AppName">Translator</system:String>
	<system:String x:Key="Shell.About">About...</system:String>
	<system:String x:Key="Shell.Error">Error!</system:String>
	<system:String x:Key="Shell.Success">Success!</system:String>
	<system:String x:Key="Shell.Connected">Internet</system:String>
	<system:String x:Key="Shell.Translate">Interactive</system:String>
	<system:String x:Key="Shell.CreateNew">New Project</system:String>
	<system:String x:Key="Shell.ProjectList">Project List</system:String>
	<system:String x:Key="Shell.RunProject">Current</system:String>
	
	<system:String x:Key="Shell.Settings">Settings</system:String>
	<system:String x:Key="Shell.Language">Language</system:String>
	<system:String x:Key="Shell.Exit">Quit</system:String>
